# Required
NOTION_TOKEN=ntn_your_token_here

# Storage
# NTN_DIR=notion

# Git commit/push
# NTN_COMMIT=true
# NTN_COMMIT_PERIOD=1m
# NTN_PUSH=true

# Remote git repository
# NTN_GIT_URL=https://github.com/your-org/your-repo.git
# NTN_GIT_PASS=ghp_xxx
# NTN_GIT_BRANCH=main

# Performance
# NTN_BLOCK_DEPTH=5
# NTN_MAX_FILE_SIZE=5MB

# Webhook server
# NTN_WEBHOOK_PORT=8080
# NTN_WEBHOOK_SECRET=your_secret
# NTN_WEBHOOK_AUTO_SYNC=true
# NTN_WEBHOOK_SYNC_DELAY=5s

# Logging
# NTN_LOG_FORMAT=text
